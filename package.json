{
  "name": "timeline",
  "packageManager": "yarn@4.2.2",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-command": "^0.2.0",
    "eslint-plugin-svelte": "^2.39.0",
    "globals": "^15.2.0",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-svelte": "^3.2.3",
    "supabase": "^1.167.4",
    "svelte": "^5.0.0-next.132",
    "turbo": "^1.13.3",
    "typescript-eslint": "^7.9.0",
    "vercel": "^34.1.14"
  },
  "scripts": {
    "build": "turbo run build",
    "db:reset": "yarn supabase db reset && yarn prisma migrate deploy",
    "dev": "turbo run dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "prisma": "yarn workspace @timeline/app prisma"
  }
}
