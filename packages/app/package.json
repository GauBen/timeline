{
  "name": "@timeline/app",
  "version": "0.0.1",
  "dependencies": {
    "@js-temporal/polyfill": "^0.4.4",
    "@prisma/client": "5.12.1",
    "@supabase/auth-helpers-sveltekit": "^0.13.0",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.42.4",
    "jose": "^5.2.4",
    "modern-normalize": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "1.6.1",
    "@sveltejs/adapter-vercel": "^5.3.0",
    "@sveltejs/kit": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "prisma": "^5.12.1",
    "sass": "^1.75.0",
    "svelte": "^5.0.0-next.104",
    "svelte-check": "^3.6.9",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "uistiti": "workspace:^",
    "vite": "^5.2.8"
  },
  "private": true,
  "scripts": {
    "build": "(test -f .env || cp .env.example .env) && prisma generate && paraglide-js compile --project ../../project.inlang --outdir ./src/paraglide && vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "preview": "vite preview",
    "postinstall": "paraglide-js compile --project ../../project.inlang --outdir ./src/paraglide"
  },
  "type": "module"
}
