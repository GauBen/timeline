{
  "name": "messages",
  "private": true,
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/messages.d.ts",
      "import": "./dist/messages.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js"
    }
  },
  "scripts": {
    "build": "paraglide-js compile --project ../../project.inlang --outdir ./dist",
    "dev": "chokidar '*.json' -c 'yarn build'"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "^2.5.0",
    "chokidar-cli": "^3.0.0"
  }
}
