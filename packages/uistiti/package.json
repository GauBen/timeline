{
  "name": "uistiti",
  "version": "0.0.1",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./global.css": "./dist/global.css",
    "./reset.css": "./dist/reset.css",
    "./utils.css": "./dist/utils.css"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "build": "yarn package && vite-node src/compiler.ts && publint",
    "check": "svelte-kit sync && svelte-check",
    "check:watch": "svelte-kit sync && svelte-check --watch",
    "dev": "vite-node --watch src/compiler.ts & svelte-package --watch --preserve-output",
    "dev:sandbox": "yarn dev & vite dev",
    "package": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "storybook": "vite-node --watch src/compiler.ts & storybook dev -p 6006"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-svelte-csf": "^5.0.7",
    "@storybook/sveltekit": "^9.0.18",
    "@sveltejs/kit": "^2.26.0",
    "@sveltejs/package": "https://pkg.pr.new/sveltejs/kit/@sveltejs/package@ebe1613",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@types/node": "^24.1.0",
    "colorjs.io": "^0.5.2",
    "modern-normalize": "^3.0.1",
    "publint": "^0.3.12",
    "sass": "^1.89.2",
    "storybook": "^9.0.18",
    "svelte": "^5.36.16",
    "svelte-check": "^4.3.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-node": "^3.2.4"
  },
  "peerDependencies": {
    "svelte": ">=5"
  }
}
