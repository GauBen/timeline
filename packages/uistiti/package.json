{
  "name": "uistiti",
  "version": "0.0.1",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./css": "./dist/index.css"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "build": "yarn package && node src/compiler.ts && publint",
    "check": "svelte-kit sync && svelte-check",
    "check:watch": "svelte-kit sync && svelte-check --watch",
    "dev": "node --watch src/compiler.ts & svelte-package --watch --preserve-output",
    "dev:sandbox": "yarn dev & vite dev",
    "package": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "storybook": "node --watch src/compiler.ts & storybook dev -p 6006"
  },
  "dependencies": {
    "@fontsource-variable/figtree": "^5.2.10",
    "@fontsource-variable/montserrat": "^5.2.8",
    "@fontsource/cookie": "^5.2.7",
    "modern-normalize": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/sveltekit": "^10.0.7",
    "@sveltejs/kit": "catalog:",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24.10.1",
    "publint": "^0.3.15",
    "sass": "^1.94.0",
    "storybook": "^10.0.7",
    "svelte": "^5.43.6",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "vite": "catalog:"
  },
  "peerDependencies": {
    "svelte": ">=5"
  }
}
